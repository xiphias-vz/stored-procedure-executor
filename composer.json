{
    "name": "xiphias/stored-procedure-executor",
    "description": "This is the template for developing and testing creation of composer packages",
    "autoload": {
        "psr-4": {
            "Xiphias\\": "src/Xiphias"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XiphiasTest\\": "tests/XiphiasTest/"
        }
    },
    "authors": [
        {
            "name": "Leon Vučemilo",
            "email": "leon.vučemilo@xiphias.hr"
        }
    ],
    "require": {},
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/testify": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
